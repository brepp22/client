---

interface Props {

}

const {} = Astro.props;

import Accordion from './Accordion.astro';
import Dropdown from './Dropdown.astro';


---

<header>
    <Accordion 
        details="Home\nContact" 
        link="/\n/contact"
    >
        <Dropdown 
            title="Contact Options" 
            items={["Email"]}
            links={['/contact/email']} 
        />
    </Accordion>
</header>

<style>
    header {
        background-color: #333; 
        padding: 1rem; 
    }

</style>